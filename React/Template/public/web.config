<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <httpProtocol>
            <customHeaders>
                <remove name="Access-Control-Max-Age" />
                <add name="Access-Control-Max-Age" value="3600" />
            </customHeaders>
        </httpProtocol>
        <defaultDocument>
        <files>
            <clear/>
            <add value="index.html"/>
        </files>
        </defaultDocument>         
    </system.webServer>
</configuration>
